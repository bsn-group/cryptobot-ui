{
  "access": [],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Futures Pnls",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "name": "Export",
                        "parameters": [
                          {
                            "name": "args",
                            "type": "Radzen.Blazor.RadzenSplitButtonItem",
                            "value": "${event}"
                          }
                        ],
                        "schema": {
                          "$ref": "#/definitions/Task",
                          "dotnetType": "System.Threading.Tasks.Task"
                        },
                        "type": "invoke-method"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginBottom": "10px",
                    "marginLeft": "0px"
                  },
                  "text": "Export",
                  "type": "splitbutton",
                  "visible": true
                },
                {
                  "buttonStyle": "light",
                  "events": {
                    "click": [
                      {
                        "code": "await pnlDataGrid.Reload();",
                        "type": "execute"
                      }
                    ]
                  },
                  "icon": "refresh",
                  "name": "refreshButton",
                  "size": "md",
                  "style": {
                    "display": "inline-block",
                    "float": "right",
                    "marginBottom": "1rem",
                    "marginLeft": "1rem",
                    "position": "static",
                    "verticalAlign": "middle"
                  },
                  "text": "",
                  "type": "button"
                },
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await pnlDataGrid.GoToPage(0);await pnlDataGrid.Reload();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                },
                {
                  "allowColumnResize": true,
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "footerTemplate": "",
                      "property": "signal_id",
                      "template": [
                        {
                          "icon": "",
                          "name": "link0",
                          "parameters": [
                            {
                              "name": "SignalId",
                              "value": "${data.SignalId}"
                            }
                          ],
                          "path": "signals",
                          "text": "${data.SignalId}",
                          "type": "link"
                        }
                      ],
                      "textAlign": "right",
                      "title": "Signal Id",
                      "visible": true,
                      "width": "60px"
                    },
                    {
                      "property": "symbol",
                      "title": "Symbol",
                      "width": "100px"
                    },
                    {
                      "property": "position_type",
                      "title": "Position Type",
                      "visible": true,
                      "width": "70px"
                    },
                    {
                      "property": "exchange_id",
                      "title": "Exchange Id",
                      "visible": false
                    },
                    {
                      "property": "strategy_pair_name",
                      "title": "Strategy",
                      "width": ""
                    },
                    {
                      "property": "signal_status",
                      "title": "Signal Status",
                      "visible": true,
                      "width": "70px"
                    },
                    {
                      "property": "position_status",
                      "title": "Position Status",
                      "width": "90px"
                    },
                    {
                      "property": "executed_buy_qty",
                      "title": "Executed Buy Qty",
                      "visible": true,
                      "width": ""
                    },
                    {
                      "property": "pending_buy_qty",
                      "title": "Pending Buy Qty",
                      "visible": false,
                      "width": ""
                    },
                    {
                      "property": "executed_sell_qty",
                      "title": "Executed Sell Qty",
                      "visible": true,
                      "width": ""
                    },
                    {
                      "property": "pending_sell_qty",
                      "title": "Pending Sell Qty",
                      "visible": false,
                      "width": ""
                    },
                    {
                      "formatString": "{0:F4}",
                      "property": "entry_price",
                      "textAlign": "right",
                      "title": "Entry Price",
                      "visible": true
                    },
                    {
                      "formatString": "{0:F4}",
                      "property": "close_price",
                      "textAlign": "right",
                      "title": "Close Price"
                    },
                    {
                      "cssClass": "",
                      "formatString": "{0:F8}",
                      "property": "pnl",
                      "template": [
                        {
                          "name": "label3",
                          "style": {
                            "color": "#B9F6CA"
                          },
                          "text": "@(String.Format(\"{0:F4} ({1:F2} %)\", cryptobotUiClientModelFuturesPnlViewModel.pnl, cryptobotUiClientModelFuturesPnlViewModel.pnl_percent))",
                          "type": "label",
                          "visible": "@(cryptobotUiClientModelFuturesPnlViewModel.pnl >= 0)"
                        },
                        {
                          "name": "label6",
                          "style": {
                            "color": "#FF8A80"
                          },
                          "text": "@(String.Format(\"{0:F4} ({1:F2} %)\", cryptobotUiClientModelFuturesPnlViewModel.pnl, cryptobotUiClientModelFuturesPnlViewModel.pnl_percent))",
                          "type": "label",
                          "visible": "@(cryptobotUiClientModelFuturesPnlViewModel.pnl < 0)"
                        }
                      ],
                      "textAlign": "right",
                      "title": "Pnl"
                    },
                    {
                      "formatString": "{0:P}",
                      "property": "pnl_percent",
                      "textAlign": "right",
                      "title": "Pnl Percent",
                      "visible": false
                    },
                    {
                      "format": "date-time",
                      "formatString": "{0:dd/MM/yy HH:mm:ss.fff zzz}",
                      "property": "entry_time",
                      "title": "Entry Time",
                      "type": "string",
                      "width": "200px"
                    },
                    {
                      "format": "date-time",
                      "formatString": "{0:dd/MM/yy HH:mm:ss.fff zzz}",
                      "property": "exit_time",
                      "title": "Exit Time",
                      "type": "string",
                      "width": "200px"
                    }
                  ],
                  "count": "${FuturesPnlsCount}",
                  "data": "${FuturesPnls}",
                  "events": {
                    "loadData": [
                      {
                        "name": "OnPnlGridLoadData",
                        "parameters": [
                          {
                            "name": "args",
                            "type": "Radzen.LoadDataArgs",
                            "value": "${event}"
                          }
                        ],
                        "schema": {
                          "$ref": "#/definitions/Task",
                          "dotnetType": "System.Threading.Tasks.Task"
                        },
                        "type": "invoke-method"
                      }
                    ],
                    "rowClick": [],
                    "rowExpand": [
                      {
                        "name": "OnPnlGridRowExpand",
                        "parameters": [
                          {
                            "name": "pnl",
                            "type": "CryptobotUi.Client.Model.FuturesPnlViewModel",
                            "value": "${event}"
                          }
                        ],
                        "schema": {
                          "$ref": "#/definitions/Task",
                          "dotnetType": "System.Threading.Tasks.Task"
                        },
                        "type": "invoke-method"
                      }
                    ]
                  },
                  "filterCaseSensitivity": "caseInsensitive",
                  "filterMode": "advanced",
                  "itemType": "CryptobotUi.Client.Model.FuturesPnlViewModel",
                  "name": "pnlDataGrid",
                  "template": "",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      }
    ]
  },
  "layout": "Main",
  "name": "Futures Pnls"
}
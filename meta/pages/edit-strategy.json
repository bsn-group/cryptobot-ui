                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${strategy}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Strategy",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "cryptodb.updateStrategy",
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${parameters.id}"
                          },
                          {
                            "name": "Strategy",
                            "value": "${strategy}"
                          }
                        ],
                        "then": [
                          {
                            "condition": "result.StatusCode != System.Net.HttpStatusCode.PreconditionFailed",
                            "result": "${strategy}",
                            "type": "close-popup"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result.StatusCode == System.Net.HttpStatusCode.PreconditionFailed",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "CryptobotUi.Models.Cryptodb.Strategy",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${strategy != null && canEdit}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "cryptodb.getStrategyByid",
        "parameters": [
          {
            "name": "id",
            "value": "${parameters.id}"
          }
        ],
        "then": [
          {
            "name": "strategy",
            "type": "set",
            "value": "${result}",
            "valueType": "CryptobotUi.Models.Cryptodb.Strategy"
          },
          {
            "name": "canEdit",
            "type": "set",
            "value": "result != null",
            "valueType": "bool"
          },
          {
            "name": "LoadInitData",
            "schema": {
              "$ref": "#/definitions/Task",
              "dotnetType": "System.Threading.Tasks.Task"
            },
            "type": "invoke-method"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Edit Strategy"
}
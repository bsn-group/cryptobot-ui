{
  "access": [],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Strategies",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "path": "add-strategy",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button"
                },
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Strategy",
                        "name": "cryptodb.getStrategies",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "name,symbol,updated_time,version,exchange_type,id,position_type,status,ETag"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event == null || event.Value == \"xlsx\"}",
                        "fileName": "Strategy",
                        "format": "xlsx",
                        "name": "cryptodb.getStrategies",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "name,symbol,updated_time,version,exchange_type,id,position_type,status,ETag"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginBottom": "10px",
                    "marginLeft": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton",
                  "visible": false
                },
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await grid0.GoToPage(0);await grid0.Reload();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": "",
                  "visible": false
                },
                {
                  "allowColumnResize": true,
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "property": "name",
                      "title": "Name"
                    },
                    {
                      "property": "symbol",
                      "title": "Symbol"
                    },
                    {
                      "property": "exchange_type",
                      "title": "Exchange Type"
                    },
                    {
                      "property": "position_type",
                      "title": "Position Type"
                    },
                    {
                      "property": "status",
                      "title": "Status"
                    },
                    {
                      "formatString": "{0:d}",
                      "property": "updated_time",
                      "title": "Updated Time"
                    },
                    {
                      "property": "version",
                      "title": "Version"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Strategy",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "cryptodb.deleteStrategy",
                                "parameters": [
                                  {
                                    "name": "id",
                                    "value": "${data.id}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          },
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "size": "sm",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "width": "70px"
                    }
                  ],
                  "count": "${getStrategiesCount}",
                  "data": "${getStrategiesResult}",
                  "editMode": "single",
                  "events": {
                    "loadData": [
                      {
                        "name": "AppState.IsBusy",
                        "type": "set",
                        "value": "true",
                        "valueType": "bool"
                      },
                      {
                        "error": [
                          {
                            "name": "AppState.IsBusy",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          },
                          {
                            "detail": "Unable to load Strategies",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "cryptodb.getStrategies",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          }
                        ],
                        "then": [
                          {
                            "name": "getStrategiesResult",
                            "type": "set",
                            "value": "${result.Value.AsODataEnumerable()}",
                            "valueType": "IEnumerable<CryptobotUi.Models.Cryptodb.Strategy>"
                          },
                          {
                            "name": "getStrategiesCount",
                            "type": "set",
                            "value": "${result.Count}",
                            "valueType": "int"
                          },
                          {
                            "name": "AppState.IsBusy",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowExpand": [
                      {
                        "global": false,
                        "name": "AppState.IsBusy",
                        "type": "set",
                        "value": "true",
                        "valueType": "bool"
                      },
                      {
                        "name": "master",
                        "type": "set",
                        "value": "${event}"
                      },
                      {
                        "error": [
                          {
                            "name": "AppState.IsBusy",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "cryptodb.getStrategyConditions",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "strategy_id eq ${event.id}"
                          }
                        ],
                        "then": [
                          {
                            "code": "event.StrategyConditions = ${result}.Value",
                            "condition": "",
                            "error": [],
                            "then": [],
                            "type": "execute"
                          },
                          {
                            "name": "AppState.IsBusy",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${event.id}"
                          }
                        ],
                        "path": "edit-strategy",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "filterMode": "advanced",
                  "itemType": "CryptobotUi.Models.Cryptodb.Strategy",
                  "name": "grid0",
                  "template": [
                    {
                      "name": "heading1",
                      "size": "H4",
                      "text": "Conditions",
                      "type": "heading"
                    },
                    {
                      "events": {
                        "click": [
                          {
                            "parameters": [
                              {
                                "name": "strategy_id",
                                "type": "integer",
                                "value": "${data.id}"
                              }
                            ],
                            "path": "add-strategy-condition",
                            "then": [
                              {
                                "name": "CreateStrategyCondition",
                                "parameters": [
                                  {
                                    "name": "strategyCondition",
                                    "type": "CryptobotUi.Models.Cryptodb.StrategyCondition",
                                    "value": "${dialogResult}"
                                  },
                                  {
                                    "name": "strategy_id",
                                    "type": "long",
                                    "value": "${data.id}"
                                  }
                                ],
                                "schema": {
                                  "$ref": "#/definitions/Task",
                                  "dotnetType": "System.Threading.Tasks.Task"
                                },
                                "type": "invoke-method"
                              },
                              {
                                "code": "await grid1.Reload();",
                                "type": "execute"
                              }
                            ],
                            "type": "popup",
                            "width": 800
                          }
                        ]
                      },
                      "icon": "add_circle_outline",
                      "name": "StrategyConditionAddButton",
                      "style": {
                        "marginBottom": "10px"
                      },
                      "text": "Add",
                      "type": "button"
                    },
                    {
                      "allowFiltering": true,
                      "allowPaging": true,
                      "allowSorting": false,
                      "columns": [
                        {
                          "property": "id",
                          "title": "Id"
                        },
                        {
                          "property": "name",
                          "title": "Name"
                        },
                        {
                          "property": "condition_group",
                          "title": "Condition group"
                        },
                        {
                          "property": "condition_sub_group",
                          "title": "Condition Sub Group"
                        },
                        {
                          "property": "category",
                          "title": "Category"
                        },
                        {
                          "property": "time_frame",
                          "title": "Time Frame"
                        },
                        {
                          "property": "last_observed",
                          "title": "Last Observed"
                        },
                        {
                          "formatString": "{0:d}",
                          "property": "created_time",
                          "title": "Created Time"
                        },
                        {
                          "property": "sequence",
                          "sortProperty": "",
                          "title": "Sequence"
                        },
                        {
                          "property": "version",
                          "title": "Version"
                        },
                        {
                          "filterable": false,
                          "sortable": false,
                          "template": [
                            {
                              "attributes": [
                                {
                                  "name": "@onclick:stopPropagation",
                                  "value": true
                                }
                              ],
                              "buttonStyle": "danger",
                              "events": {
                                "click": [
                                  {
                                    "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                    "error": [
                                      {
                                        "detail": "Unable to delete Strategy",
                                        "severity": "error",
                                        "summary": "Error",
                                        "type": "notify"
                                      }
                                    ],
                                    "name": "cryptodb.deleteStrategyCondition",
                                    "parameters": [
                                      {
                                        "name": "id",
                                        "value": "${data.id}"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "code": "await grid1.Reload();",
                                        "condition": "result != null",
                                        "type": "execute"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "icon": "close",
                              "name": "StrategyConditionDeleteButton",
                              "size": "sm",
                              "text": "",
                              "type": "button"
                            }
                          ],
                          "textAlign": "center",
                          "width": "70px"
                        }
                      ],
                      "data": "${data.StrategyConditions}",
                      "editMode": "single",
                      "emptyText": "No conditions. Click (+) to add.",
                      "events": {
                        "rowSelect": []
                      },
                      "filterMode": "advanced",
                      "itemType": "CryptobotUi.Models.Cryptodb.StrategyCondition",
                      "name": "grid1",
                      "type": "grid"
                    }
                  ],
                  "title": "",
                  "type": "grid",
                  "value": ""
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      }
    ]
  },
  "layout": "Main",
  "name": "Strategies"
}
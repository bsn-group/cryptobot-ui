        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "cryptodb.getMarketEventByid",
        "parameters": [
          {
            "name": "id",
            "value": "${parameters.id}"
          }
        ],
        "then": [
          {
            "name": "marketevent",
            "type": "set",
            "value": "${result}",
            "valueType": "CryptobotUi.Models.Cryptodb.MarketEvent"
          },
          {
            "name": "canEdit",
            "type": "set",
            "value": "result != null",
            "valueType": "bool"
          }
        ],
        "type": "invoke"
      }
    ]

                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update MarketEvent",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "cryptodb.updateMarketEvent",
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${parameters.id}"
                          },
                          {
                            "name": "MarketEvent",
                            "value": "${marketevent}"
                          }
                        ],
                        "then": [
                          {
                            "condition": "result.StatusCode != System.Net.HttpStatusCode.PreconditionFailed",
                            "result": "${marketevent}",
                            "type": "close-popup"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result.StatusCode == System.Net.HttpStatusCode.PreconditionFailed",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }

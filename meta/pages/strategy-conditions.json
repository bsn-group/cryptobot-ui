{
  "access": [],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H4",
          "text": "Conditions",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "path": "add-strategy-condition",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button"
                },
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Strategy Conditions",
                        "name": "cryptodb.getStrategyConditions",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": "Strategy"
                          },
                          {
                            "name": "$select",
                            "value": "category,created_time,name,condition_group,sequence,time_frame,id,Strategy.symbol,version,last_observed"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event == null || event.Value == \"xlsx\"}",
                        "fileName": "Strategy Conditions",
                        "format": "xlsx",
                        "name": "cryptodb.getStrategyConditions",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": "Strategy"
                          },
                          {
                            "name": "$select",
                            "value": "category,created_time,name,condition_group,sequence,time_frame,id,Strategy.symbol,version,last_observed"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginBottom": "10px",
                    "marginLeft": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton"
                },
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await grid0.GoToPage(0);await grid0.Reload();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                },
                {
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "property": "category",
                      "title": "Category"
                    },
                    {
                      "formatString": "{0:dd/MM/yy HH:mm:ss zzz}",
                      "property": "created_time",
                      "title": "Created Time"
                    },
                    {
                      "property": "name",
                      "title": "Name"
                    },
                    {
                      "property": "condition_group",
                      "title": "Condition Group"
                    },
                    {
                      "format": "int64",
                      "property": "condition_sub_group",
                      "title": "Condition Sub Group",
                      "type": "integer"
                    },
                    {
                      "property": "sequence",
                      "title": "Sequence"
                    },
                    {
                      "property": "time_frame",
                      "title": "Time Frame"
                    },
                    {
                      "property": "id",
                      "title": "Id"
                    },
                    {
                      "filterProperty": "Strategy.symbol",
                      "property": "strategy_id",
                      "sortProperty": "Strategy.symbol",
                      "template": "${data.Strategy?.symbol}",
                      "title": "Strategy"
                    },
                    {
                      "property": "version",
                      "title": "Version"
                    },
                    {
                      "property": "last_observed",
                      "title": "Last Observed"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete StrategyCondition",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "cryptodb.deleteStrategyCondition",
                                "parameters": [
                                  {
                                    "name": "id",
                                    "value": "${data.id}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          },
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "size": "sm",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "width": "70px"
                    }
                  ],
                  "count": "${getStrategyConditionsCount}",
                  "data": "${getStrategyConditionsResult}",
                  "events": {
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load StrategyConditions",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "cryptodb.getStrategyConditions",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "operator": "and",
                            "value": [
                              {
                                "filters": [
                                  {
                                    "operation": "contains",
                                    "property": "category",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "name",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "condition_group",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  }
                                ],
                                "operator": "or"
                              },
                              {
                                "value": "${(string.IsNullOrEmpty(event.filter)? \"true\" : event.filter)}"
                              }
                            ]
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Strategy"
                          }
                        ],
                        "then": [
                          {
                            "name": "getStrategyConditionsResult",
                            "type": "set",
                            "value": "${result.Value.AsODataEnumerable()}",
                            "valueType": "IEnumerable<CryptobotUi.Models.Cryptodb.StrategyCondition>"
                          },
                          {
                            "name": "getStrategyConditionsCount",
                            "type": "set",
                            "value": "${result.Count}",
                            "valueType": "int"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": []
                  },
                  "filterMode": "advanced",
                  "itemType": "CryptobotUi.Models.Cryptodb.StrategyCondition",
                  "name": "grid0",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      }
    ]
  },
  "layout": "Main",
  "name": "Strategy Conditions"
}